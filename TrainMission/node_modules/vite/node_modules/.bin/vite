#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules/vite/bin/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules/vite/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules/vite/bin/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules/vite/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/vite@5.4.14_less@4.2.2_sass@1.86.0/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
