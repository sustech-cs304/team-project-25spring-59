#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/sass@1.86.0/node_modules/sass/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/sass@1.86.0/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/sass@1.86.0/node_modules/sass/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/sass@1.86.0/node_modules:/mnt/c/Users/bdxly/Desktop/team-project-25spring-59/TrainMission/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/sass@1.86.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../.pnpm/sass@1.86.0/node_modules/sass/sass.js" "$@"
fi
